{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es2015",
    "sourceMap": true,
    "skipLibCheck": true,
    //"strict": true,
    "types": [ "webpack-env" ],

    "plugins": [
      {
        "name": "typescript-lit-html-plugin",
        "name": "lit-element"
      }
    ]
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
